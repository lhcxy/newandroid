<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">
    <root-path
        name="root"
        path="phonelive/" />
    <files-path
        name="files"
        path="phonelive/" />

    <cache-path
        name="cache"
        path="phonelive/" />
    <external-files-path
        name="external_file_path"
        path="phonelive/" />
    <external-cache-path
        name="external_cache_path"
        path="phonelive/" />
    <!--external-path标签用来指定Uri共享的，name属性的值可以自定义，
    path属性的值表示共享的具体位置，设置为空，就表示共享整个SD卡-->
    <external-path
        name="external"
        path="phonelive/" />

    <external-path
        name="images"
        path="phonelive/" />
    <external-path
        name="beta_external_path"
        path="Download/" />
    <external-path
        name="app_down"
        path="download/" />
    <!--/storage/emulated/0/Android/data/${applicationId}/files/apk/-->
    <external-path
        name="beta_external_files_path"
        path="Android/data/" />
        <external-path
            name="camera_photos"
            path="" />
</paths>
